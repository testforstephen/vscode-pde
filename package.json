{
  "name": "vscode-pde",
  "displayName": "Eclipse PDE support ",
  "description": "Adds Eclipse PDE Support to Visual Studio Code",
  "version": "0.3.1",
  "publisher": "yaozheng",
  "preview": true,
  "keywords": [
    "java",
    "eclipse",
    "PDE"
  ],
  "engines": {
    "vscode": "^1.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/testforstephen/vscode-pde"
  },
  "bugs": {
    "url": "https://github.com/testforstephen/vscode-pde/issues"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "activationEvents": [
    "onLanguage:java",
    "workspaceContains:pom.xml",
    "workspaceContains:build.gradle",
    "workspaceContains:.classpath",
    "onCommand:java.pde.debug",
    "onCommand:java.pde.reload"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "javaExtensions": [
      "./server/org.sat4j.pb_2.3.5.v201404071733.jar",
      "./server/org.sat4j.core_2.3.5.v201308161310.jar",
      "./server/org.eclipse.ecf.identity_3.9.1.v20181107-1749.jar",
      "./server/org.eclipse.ecf.provider.filetransfer_3.2.400.v20180306-0429.jar",
      "./server/org.eclipse.equinox.concurrent_1.1.300.v20190514-1046.jar",
      "./server/org.eclipse.ecf_3.9.3.v20190423-0625.jar",
      "./server/org.eclipse.ecf.filetransfer_5.0.100.v20180301-0132.jar",
      "./server/org.apache.felix.scr_2.1.14.v20190123-1619.jar",
      "./server/org.eclipse.equinox.p2.transport.ecf_1.2.100.v20180822-1354.jar",
      "./server/org.tukaani.xz_1.8.0.v20180207-1613.jar",
      "./server/org.eclipse.equinox.p2.engine_2.6.300.v20190318-1320.jar",
      "./server/org.eclipse.equinox.p2.repository_2.4.400.v20190318-1320.jar",
      "./server/org.eclipse.equinox.ds_1.6.0.v20190122-0806.jar",
      "./server/org.eclipse.equinox.p2.metadata.repository_1.3.100.v20180822-1302.jar",
      "./server/org.eclipse.equinox.p2.metadata_2.4.400.v20190409-0548.jar",
      "./server/org.eclipse.equinox.p2.garbagecollector_1.1.100.v20180822-1302.jar",
      "./server/org.eclipse.equinox.p2.jarprocessor_1.1.200.v20180921-1009.jar",
      "./server/org.eclipse.equinox.p2.artifact.repository_1.3.100.v20190330-1646.jar",
      "./server/org.eclipse.equinox.p2.director_2.4.300.v20190215-2242.jar",
      "./server/org.eclipse.equinox.p2.director.app_1.1.200.v20180822-1302.jar",
      "./server/org.eclipse.equinox.p2.publisher.eclipse_1.3.200.v20190329-1624.jar",
      "./server/org.eclipse.equinox.p2.publisher_1.5.100.v20180822-1302.jar",
      "./server/org.eclipse.pde.build_3.10.400.v20190320-1020.jar",
      "./server/org.eclipse.update.configurator_3.4.300.v20190518-1030.jar",
      "./server/org.eclipse.team.core_3.8.600.v20190519-2354.jar",
      "./server/org.eclipse.equinox.p2.repository.tools_2.2.200.v20181102-0649.jar",
      "./server/org.eclipse.equinox.p2.core_2.6.0.v20190215-2242.jar",
      "./server/org.eclipse.pde.core_3.13.0.v20190513-1223.jar",
      "./server/org.eclipse.pde.launching_3.7.600.v20190513-1223.jar",
      "./server/org.eclipse.equinox.p2.touchpoint.natives_1.3.200.v20180921-1019.jar",
      "./server/org.eclipse.equinox.p2.touchpoint.eclipse_2.2.300.v20190215-2242.jar",
      "./server/org.eclipse.jdt.ls.importer.pde_1.2.1.20190906-0909.jar",
      "./server/org.eclipse.e4.core.contexts_1.8.100.v20190518-1217.jar",
      "./server/org.eclipse.e4.core.services_2.1.400.v20181215-2145.jar",
      "./server/org.eclipse.e4.core.di_1.7.200.v20190506-1457.jar",
      "./server/org.eclipse.e4.core.di.annotations_1.6.400.v20190518-1217.jar",
      "./server/org.eclipse.ecf.provider.filetransfer.httpclient45_1.0.0.v20190530-1947.jar",
      "./server/org.sonatype.tycho.m2e_0.8.1.201704211436.jar",
      "./server/org.apache.httpcomponents.httpcore_4.4.10.v20190123-2214.jar",
      "./server/org.apache.httpcomponents.httpclient_4.5.6.v20190503-0009.jar",
      "./server/org.apache.commons.codec_1.10.0.v20180409-1845.jar",
      "./server/org.apache.commons.logging_1.2.0.v20180409-1502.jar",
      "./server/org.sonatype.tycho.m2e_0.8.1.201704211436.jar",
      "./server/org.eclipse.jdt.junit4.runtime_1.1.1000.v20190510-0840.jar",
      "./server/org.eclipse.pde.junit.runtime_3.5.400.v20190513-1223.jar",
      "./server/javax.annotation_1.2.0.v201602091430.jar"
    ],
    "commands": [
      {
        "command": "java.pde.debug",
        "title": "Debug PDE application"
      },
      {
        "command": "java.pde.reload",
        "title": "Reload Target Platform"
      },
      {
        "command": "java.pde.runUnitTest",
        "title": "Run JUnit Plug-in Test"
      },
      {
        "command": "java.pde.debugUnitTest",
        "title": "Debug JUnit Plug-in Test"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "java.pde.debug",
          "when": "resourceExtname == .launch",
          "group": "pdedebug"
        },
        {
          "command": "java.pde.reload",
          "when": "resourceExtname == .target",
          "group": "pdedebug"
        },
        {
          "command": "java.pde.runUnitTest",
          "when": "resourceExtname == .java",
          "group": "pdedebug@0"
        },
        {
          "command": "java.pde.debugUnitTest",
          "when": "resourceExtname == .java",
          "group": "pdedebug@1"
        },
        {
          "command": "java.pde.runUnitTest",
          "when": "javaLSReady && explorerResourceIsFolder",
          "group": "pdedebug@0"
        },
        {
          "command": "java.pde.debugUnitTest",
          "when": "javaLSReady && explorerResourceIsFolder",
          "group": "pdedebug@1"
        }
      ],
      "editor/context": [
        {
          "command": "java.pde.debug",
          "when": "resourceExtname == .launch",
          "group": "pdedebug"
        },
        {
          "command": "java.pde.reload",
          "when": "resourceExtname == .target",
          "group": "pdedebug"
        },
        {
          "command": "java.pde.runUnitTest",
          "when": "resourceExtname == .java",
          "group": "pdedebug@0"
        },
        {
          "command": "java.pde.debugUnitTest",
          "when": "resourceExtname == .java",
          "group": "pdedebug@1"
        }
      ],
      "view/item/context": [
        {
          "command": "java.pde.runUnitTest",
          "when": "view == testExplorer",
          "group": "testExplorer@2"
        },
        {
          "command": "java.pde.debugUnitTest",
          "when": "view == testExplorer",
          "group": "testExplorer@3"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test",
    "build-server": "./node_modules/.bin/gulp build_server",
    "full-build": "./node_modules/.bin/gulp full_build"
  },
  "extensionDependencies": [
    "redhat.java"
  ],
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.12",
    "gulp": "^4.0.0",
    "gulp-copy": "^4.0.1",
    "gulp-download2": "^1.0.2",
    "gulp-tslint": "^8.1.3",
    "mocha": "^5.2.0",
    "tslint": "^5.11.0",
    "typescript": "^3.2.1",
    "vscode": "^1.1.36"
  },
  "dependencies": {}
}