{
  "name": "vscode-pde",
  "displayName": "Eclipse PDE support ",
  "description": "Adds Eclipse PDE Support to Visual Studio Code",
  "version": "0.10.2",
  "publisher": "yaozheng",
  "preview": true,
  "keywords": [
    "java",
    "eclipse",
    "PDE"
  ],
  "engines": {
    "vscode": "^1.59.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/testforstephen/vscode-pde"
  },
  "bugs": {
    "url": "https://github.com/testforstephen/vscode-pde/issues"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "activationEvents": [
    "onLanguage:java",
    "workspaceContains:pom.xml",
    "workspaceContains:build.gradle",
    "workspaceContains:.classpath",
    "onCommand:java.pde.debug",
    "onCommand:java.pde.reload"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "javaExtensions": [
      "./server/org.sat4j.pb_2.3.6.v20201214.jar",
      "./server/org.sat4j.core_2.3.6.v20201214.jar",
      "./server/bcpg_1.78.1.jar",
      "./server/bcprov_1.78.1.jar",
      "./server/bcutil_1.78.1.jar",
      "./server/org.eclipse.ecf.identity_3.10.0.v20230422-0242.jar",
      "./server/org.eclipse.ecf.provider.filetransfer_3.3.0.v20230422-0242.jar",
      "./server/org.eclipse.equinox.concurrent_1.3.100.v20240514-0729.jar",
      "./server/org.eclipse.ecf_3.11.0.v20230507-1923.jar",
      "./server/org.eclipse.ecf.filetransfer_5.1.103.v20230705-0614.jar",
      "./server/org.eclipse.equinox.p2.transport.ecf_1.4.300.v20240321-1450.jar",
      "./server/org.tukaani.xz_1.9.0.jar",
      "./server/org.eclipse.equinox.p2.engine_2.10.200.v20240425-1316.jar",
      "./server/org.eclipse.equinox.p2.repository_2.9.100.v20240511-1722.jar",
      "./server/org.eclipse.equinox.p2.metadata.repository_1.5.400.v20240515-1919.jar",
      "./server/org.eclipse.equinox.p2.metadata_2.9.100.v20240416-0654.jar",
      "./server/org.eclipse.equinox.p2.garbagecollector_1.3.300.v20240321-1450.jar",
      "./server/org.eclipse.equinox.p2.jarprocessor_1.3.400.v20240321-1450.jar",
      "./server/org.eclipse.equinox.p2.artifact.repository_1.5.400.v20240416-0654.jar",
      "./server/org.eclipse.equinox.p2.director_2.6.400.v20240321-1450.jar",
      "./server/org.eclipse.equinox.p2.publisher.eclipse_1.6.100.v20240519-2013.jar",
      "./server/org.eclipse.equinox.p2.publisher_1.9.200.v20240429-0755.jar",
      "./server/org.eclipse.pde.build_3.12.400.v20240515-2132.jar",
      "./server/org.eclipse.team.core_3.10.400.v20240413-1529.jar",
      "./server/org.eclipse.equinox.p2.repository.tools_2.4.400.v20240416-0654.jar",
      "./server/org.eclipse.equinox.p2.core_2.12.0.v20240515-1919.jar",
      "./server/org.eclipse.pde.core_3.18.100.v20240531-0649.jar",
      "./server/org.eclipse.pde.launching_3.13.0.v20240424-1246.jar",
      "./server/org.eclipse.equinox.p2.touchpoint.natives_1.5.300.v20240423-0755.jar",
      "./server/org.eclipse.equinox.p2.touchpoint.eclipse_2.4.300.v20240511-1722.jar",
      "./server/org.eclipse.jdt.ls.importer.pde_1.4.3.20240729-1344.jar",
      "./server/org.eclipse.e4.core.contexts_1.12.600.v20240413-1529.jar",
      "./server/org.eclipse.e4.core.services_2.4.400.v20240413-1529.jar",
      "./server/org.eclipse.e4.core.di_1.9.400.v20240414-1643.jar",
      "./server/org.eclipse.e4.core.di.annotations_1.8.400.v20240413-1529.jar",
      "./server/org.eclipse.ecf.provider.filetransfer.httpclient5_1.1.0.v20230423-0417.jar",
      "./server/org.apache.httpcomponents.client5.httpclient5_5.2.1.v20230802-0806.jar",
      "./server/org.apache.httpcomponents.core5.httpcore5_5.2.3.v20230922-1600.jar",
      "./server/org.apache.httpcomponents.core5.httpcore5-h2_5.2.3.v20230922-1600.jar",
      "./server/org.eclipse.jdt.junit4.runtime_1.3.100.v20231214-1952.jar",
      "./server/org.eclipse.pde.junit.runtime_3.8.100.v20240130-1723.jar",
      "./server/org.osgi.service.repository_1.1.0.201505202024.jar",
      "./server/org.eclipse.m2e.pde.target_2.0.501.20240411-1122.jar",
      "./server/biz.aQute.bnd.util_7.0.0.202310060912.jar",
      "./server/biz.aQute.bndlib_7.0.0.202310060912.jar",
      "./server/org.apache.commons.commons-io_2.16.1.jar",
      "./server/javax.annotation_1.3.5.v20200909-1856.jar"
    ],
    "commands": [
      {
        "command": "java.pde.debug",
        "title": "Debug PDE application"
      },
      {
        "command": "java.pde.reload",
        "title": "Reload Target Platform"
      },
      {
        "command": "java.pde.runUnitTest",
        "title": "Run JUnit Plug-in Test"
      },
      {
        "command": "java.pde.debugUnitTest",
        "title": "Debug JUnit Plug-in Test"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "java.pde.debug",
          "when": "resourceExtname == .launch",
          "group": "pdedebug"
        },
        {
          "command": "java.pde.reload",
          "when": "resourceExtname == .target",
          "group": "pdedebug"
        },
        {
          "command": "java.pde.runUnitTest",
          "when": "resourceExtname == .java",
          "group": "pdedebug@0"
        },
        {
          "command": "java.pde.debugUnitTest",
          "when": "resourceExtname == .java",
          "group": "pdedebug@1"
        },
        {
          "command": "java.pde.runUnitTest",
          "when": "javaLSReady && explorerResourceIsFolder",
          "group": "pdedebug@0"
        },
        {
          "command": "java.pde.debugUnitTest",
          "when": "javaLSReady && explorerResourceIsFolder",
          "group": "pdedebug@1"
        }
      ],
      "editor/context": [
        {
          "command": "java.pde.debug",
          "when": "resourceExtname == .launch",
          "group": "pdedebug"
        },
        {
          "command": "java.pde.reload",
          "when": "resourceExtname == .target",
          "group": "pdedebug"
        },
        {
          "command": "java.pde.runUnitTest",
          "when": "resourceExtname == .java",
          "group": "pdedebug@0"
        },
        {
          "command": "java.pde.debugUnitTest",
          "when": "resourceExtname == .java",
          "group": "pdedebug@1"
        }
      ],
      "testing/item/context": [
        {
          "command": "java.pde.runUnitTest",
          "group": "testExplorer@0"
        },
        {
          "command": "java.pde.debugUnitTest",
          "group": "testExplorer@1"
        }
      ],
      "testing/item/gutter": [
        {
          "command": "java.pde.runUnitTest",
          "group": "pdeUnitTest@0"
        },
        {
          "command": "java.pde.debugUnitTest",
          "group": "pdeUnitTest@1"
        }
      ]
    },
    "configuration": {
      "title": "Eclipse PDE",
      "properties": {
        "java.pde.test.launch.useUIThread": {
          "type": "boolean",
          "default": false,
          "description": "Launch plugin unit tests using SWT UI thread.",
          "scope": "machine-overridable"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "build-server": "./node_modules/.bin/gulp build_server",
    "full-build": "./node_modules/.bin/gulp full_build"
  },
  "extensionDependencies": [
    "redhat.java"
  ],
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^8.10.51",
    "@types/vscode": "1.59.0",
    "download": "^7.1.0",
    "gulp": "^4.0.0",
    "gulp-copy": "^4.0.1",
    "gulp-tslint": "^8.1.3",
    "mocha": "^10.1.0",
    "tslint": "^5.11.0",
    "typescript": "^3.2.1"
  },
  "dependencies": {
    "compare-versions": "^3.6.0"
  }
}