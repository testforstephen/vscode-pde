{
  "name": "vscode-pde",
  "displayName": "Eclipse PDE support ",
  "description": "Adds Eclipse PDE Support to Visual Studio Code",
  "version": "0.11.1",
  "publisher": "yaozheng",
  "preview": true,
  "keywords": [
    "java",
    "eclipse",
    "PDE"
  ],
  "engines": {
    "vscode": "^1.59.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/testforstephen/vscode-pde"
  },
  "bugs": {
    "url": "https://github.com/testforstephen/vscode-pde/issues"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "activationEvents": [
    "onLanguage:java",
    "workspaceContains:pom.xml",
    "workspaceContains:build.gradle",
    "workspaceContains:.classpath",
    "onCommand:java.pde.debug",
    "onCommand:java.pde.reload"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "javaExtensions": [
      "./server/org.sat4j.pb_2.3.6.v20201214.jar",
      "./server/org.sat4j.core_2.3.6.v20201214.jar",
      "./server/bcpg_1.81.0.jar",
      "./server/bcprov_1.81.0.jar",
      "./server/bcutil_1.81.0.jar",
      "./server/org.eclipse.ecf.identity_3.10.0.v20240812-1535.jar",
      "./server/org.eclipse.ecf.provider.filetransfer_3.3.100.v20250115-0406.jar",
      "./server/org.eclipse.equinox.concurrent_1.3.400.v20250715-0436.jar",
      "./server/org.eclipse.ecf_3.13.0.v20250304-2338.jar",
      "./server/org.eclipse.ecf.filetransfer_5.1.103.v20230705-0614.jar",
      "./server/org.eclipse.equinox.p2.transport.ecf_1.4.500.v20250519-0547.jar",
      "./server/org.tukaani.xz_1.10.0.jar",
      "./server/org.eclipse.equinox.p2.engine_2.10.600.v20250724-1029.jar",
      "./server/org.eclipse.equinox.p2.repository_2.9.500.v20250721-0421.jar",
      "./server/org.eclipse.equinox.p2.metadata.repository_1.5.700.v20250720-0955.jar",
      "./server/org.eclipse.equinox.p2.metadata_2.9.500.v20250726-1041.jar",
      "./server/org.eclipse.equinox.p2.garbagecollector_1.3.700.v20250715-0459.jar",
      "./server/org.eclipse.equinox.p2.jarprocessor_1.3.600.v20250517-0338.jar",
      "./server/org.eclipse.equinox.p2.artifact.repository_1.5.800.v20250620-1926.jar",
      "./server/org.eclipse.equinox.p2.director_2.6.800.v20250714-1236.jar",
      "./server/org.eclipse.equinox.p2.publisher.eclipse_1.6.600.v20250720-1525.jar",
      "./server/org.eclipse.equinox.p2.publisher_1.9.600.v20250720-0951.jar",
      "./server/org.eclipse.pde.build_3.12.800.v20250515-0456.jar",
      "./server/org.eclipse.team.core_3.10.800.v20250610-0708.jar",
      "./server/org.eclipse.equinox.p2.repository.tools_2.4.800.v20250720-1525.jar",
      "./server/org.eclipse.equinox.p2.core_2.13.100.v20250317-0903.jar",
      "./server/org.eclipse.pde.core_3.21.0.v20250813-2022.jar",
      "./server/org.eclipse.pde.launching_3.13.500.v20250715-1504.jar",
      "./server/org.eclipse.equinox.p2.touchpoint.natives_1.5.700.v20250721-0422.jar",
      "./server/org.eclipse.equinox.p2.touchpoint.eclipse_2.4.600.v20250722-0413.jar",
      "./server/org.eclipse.jdt.ls.importer.pde_1.4.3.20250904-2044.jar",
      "./server/org.eclipse.e4.core.contexts_1.13.200.v20250609-0437.jar",
      "./server/org.eclipse.e4.core.services_2.5.200.v20250326-1945.jar",
      "./server/org.eclipse.e4.core.di_1.9.700.v20250609-0907.jar",
      "./server/org.eclipse.e4.core.di.annotations_1.8.400.v20240413-1529.jar",
      "./server/org.eclipse.ecf.provider.filetransfer.httpclient5_1.1.100.v20250108-1923.jar",
      "./server/org.apache.httpcomponents.client5.httpclient5_5.5.0.v20250522-2300.jar",
      "./server/org.apache.httpcomponents.core5.httpcore5_5.3.4.v20250320-1400.jar",
      "./server/org.apache.httpcomponents.core5.httpcore5-h2_5.3.4.v20250320-1400.jar",
      "./server/org.eclipse.jdt.junit4.runtime_1.3.100.v20231214-1952.jar",
      "./server/org.eclipse.pde.junit.runtime_3.8.200.v20250624-0439.jar",
      "./server/org.osgi.service.repository_1.1.0.201505202024.jar",
      "./server/org.eclipse.m2e.pde.target_2.1.3.20250709-0351.jar",
      "./server/biz.aQute.bnd.util_7.1.0.202411251545.jar",
      "./server/biz.aQute.bndlib_7.1.0.202411251545.jar",
      "./server/bndtools.api_7.1.0.202411251545.jar",
      "./server/org.bndtools.versioncontrol.ignores.manager_7.1.0.202411251545.jar",
      "./server/jakarta.annotation-api_2.1.1.jar",
      "./server/jakarta.inject.jakarta.inject-api_2.0.1.jar",
      "./server/org.apache.commons.commons-io_2.20.0.jar",
      "./server/javax.annotation_1.3.5.v20200504-1837.jar",
      "./server/javax.inject_1.0.0.v20091030.jar"
    ],
    "commands": [
      {
        "command": "java.pde.debug",
        "title": "Debug PDE application"
      },
      {
        "command": "java.pde.reload",
        "title": "Reload Target Platform"
      },
      {
        "command": "java.pde.runUnitTest",
        "title": "Run JUnit Plug-in Test"
      },
      {
        "command": "java.pde.debugUnitTest",
        "title": "Debug JUnit Plug-in Test"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "java.pde.debug",
          "when": "resourceExtname == .launch",
          "group": "pdedebug"
        },
        {
          "command": "java.pde.reload",
          "when": "resourceExtname == .target",
          "group": "pdedebug"
        },
        {
          "command": "java.pde.runUnitTest",
          "when": "resourceExtname == .java",
          "group": "pdedebug@0"
        },
        {
          "command": "java.pde.debugUnitTest",
          "when": "resourceExtname == .java",
          "group": "pdedebug@1"
        },
        {
          "command": "java.pde.runUnitTest",
          "when": "javaLSReady && explorerResourceIsFolder",
          "group": "pdedebug@0"
        },
        {
          "command": "java.pde.debugUnitTest",
          "when": "javaLSReady && explorerResourceIsFolder",
          "group": "pdedebug@1"
        }
      ],
      "editor/context": [
        {
          "command": "java.pde.debug",
          "when": "resourceExtname == .launch",
          "group": "pdedebug"
        },
        {
          "command": "java.pde.reload",
          "when": "resourceExtname == .target",
          "group": "pdedebug"
        },
        {
          "command": "java.pde.runUnitTest",
          "when": "resourceExtname == .java",
          "group": "pdedebug@0"
        },
        {
          "command": "java.pde.debugUnitTest",
          "when": "resourceExtname == .java",
          "group": "pdedebug@1"
        }
      ],
      "testing/item/context": [
        {
          "command": "java.pde.runUnitTest",
          "group": "testExplorer@0"
        },
        {
          "command": "java.pde.debugUnitTest",
          "group": "testExplorer@1"
        }
      ],
      "testing/item/gutter": [
        {
          "command": "java.pde.runUnitTest",
          "group": "pdeUnitTest@0"
        },
        {
          "command": "java.pde.debugUnitTest",
          "group": "pdeUnitTest@1"
        }
      ]
    },
    "configuration": {
      "title": "Eclipse PDE",
      "properties": {
        "java.pde.test.launch.useUIThread": {
          "type": "boolean",
          "default": false,
          "description": "Launch plugin unit tests using SWT UI thread.",
          "scope": "machine-overridable"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "build-server": "./node_modules/.bin/gulp build_server",
    "full-build": "./node_modules/.bin/gulp full_build"
  },
  "extensionDependencies": [
    "redhat.java"
  ],
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^8.10.51",
    "@types/vscode": "1.59.0",
    "download": "^7.1.0",
    "gulp": "^4.0.0",
    "gulp-copy": "^4.0.1",
    "gulp-tslint": "^8.1.3",
    "mocha": "^10.1.0",
    "tslint": "^5.11.0",
    "typescript": "^3.2.1"
  },
  "dependencies": {
    "compare-versions": "^3.6.0"
  }
}